#ifndef DEQUE_H
#define DEQUE_H

#include "jniobject.h"
#include "node.h"

namespace world {

class Deque : public jni::JNIObject {
private:
    Q_DISABLE_COPY(Deque)
    Node* sentinel;
    Node* current;
public:
    Deque(jobject obj) : JNIObject(obj){}
    Node* getSentinel(bool useCache = true);
    Node* getCurrent(bool useCache = true);
};

}

#endif // DEQUE_H
